<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="group_0700.flighthelper" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".client_menu" >
            <meta-data
                android:name="android.support.MainActivity"
                android:value="group_0700.flighthelper.MainActivity" />
        </activity>
        <activity android:name=".admin_menu" >
            <meta-data
                android:name="android.support.MainActivity"
                android:value="group_0700.flighthelper.MainActivity" />
        </activity>
        <activity android:name=".BookedItineraryView" >
        </activity>
        <activity android:name=".ViewAllClientsInfo" >
        </activity>
        <activity android:name=".ViewPersonalInfor" >
        </activity>
    </application>

</manifest>
